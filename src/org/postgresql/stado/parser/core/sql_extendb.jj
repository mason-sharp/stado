void FromTableSpec(PrintStream prn):
{}
{

	TableSpec(prn) (
			<CROSS_> <JOIN_> TableSpec(prn)
			|
   		    (
			  [<INNER_>] <JOIN_> TableSpec(prn) JoinSpec(prn)
			|
              (
               <LEFT_>
   		   	   |
		       <RIGHT_>
               | 
               <FULL_> 
		   	  )
			  [<OUTER_>] <JOIN_> TableSpec(prn) JoinSpec(prn)
		    )
		    |
		    <NATURAL_>
		     (
			  [<INNER_>] <JOIN_> TableSpec(prn)
			  |
              (
               <LEFT_>
   		   	   |
		       <RIGHT_>
               | 
               <FULL_> 
		   	  )
			  [<OUTER_>] <JOIN_> TableSpec(prn)
			 )
		     
			)*

}

