	(
	    <QUOTE_STRING_> [ <AS_> ] <STRING_LITERAL> |
	    <ESCAPE_> [ <AS_> ] <STRING_LITERAL> |
	    <FORCE_QUOTE_> ColumnNameList(prn) |
	    <FORCE_NOT_NULL_> ColumnNameList(prn)
    )*
}

