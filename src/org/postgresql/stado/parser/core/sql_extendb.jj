void CopyData(PrintStream prn):
{}
{
    <COPY_>
    (
        TableName(prn) [ColumnNameListWithParenthesis(prn)]
        |
        <PARENTHESIS_START_> Select(prn) <PARENTHESIS_CLOSE_>
    )
    (
        <FROM_> 
        (<STDIN_> | <STRING_LITERAL>)
        | 
        <TO_> 
        (<STDOUT_> | <STRING_LITERAL>)
    )
	[
		[<WITH_>]
		(
			FormatDefOIDS(prn) |
			FormatDefDelimiter(prn) |
			FormatDefNull(prn) |
			FormatDefCSV(prn)
		)+
	]
}

